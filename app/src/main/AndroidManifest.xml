            android:icon="@mipmap/icon2"
            android:label="Aplikasi Jadwal Sholat"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.cloudidev.sugengsitungkir.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.cloudidev.sugengsitungkir.Activity_Jadwal"/>
        <activity android:name="com.cloudidev.sugengsitungkir.Activity_Halal"/>
        <activity android:name="com.cloudidev.sugengsitungkir.Activity_DoaHarian">

        </activity>
        <activity android:label="Dzikir Sore" android:name="com.cloudidev.sugengsitungkir.SoreActivity" />
        <activity android:label="Dzikir Pagi" android:name="com.cloudidev.sugengsitungkir.PagiActivity" />

    </application>

</manifest>